{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "FreelanceAI": "Debug",
      "FreelanceAI.ApiRouter": "Debug",
      "FreelanceAI.ApiRouter.Providers": "Debug"
    },
    "Console": {
      "IncludeScopes": true,
      "TimestampFormat": "yyyy-MM-dd HH:mm:ss "
    }
  },
  "AllowedHosts": "*",
  "Router": {
    "DailyBudget": 5.0,
    "MaxRetries": 3,
    "HealthCheckInterval": "00:05:00",
    "EnableCostTracking": true,
    "EnableRateLimiting": true
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5000"
      }
    },
  },
  "Groq": {
    "ApiKey": "REPLACE_WITH_LOCAL-TODO_USE_PROVIDER",
    "BaseUrl": "https://api.groq.com/openai/v1/",
    "Model": "llama-3.3-70b-versatile",
    "MaxTokens": 32768,
    "Timeout": "00:02:00",
    "Enabled": true
  },
  "Ollama": {
    "BaseUrl": "http://localhost:11434",
    "Model": "llama2",
    "MaxTokens": 4096,
    "Timeout": "00:05:00",
    "Enabled": false
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:3000",
      "http://localhost:5173",
      "http://localhost:8080"
    ],
    "AllowedMethods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
    "AllowedHeaders": ["*"],
    "AllowCredentials": true
  },
  "JsonFileServiceOptions": {
    "FilePath": "freelance-ai-responses.json",
    "MaxFileSizeInBytes": "5 * 1024 * 1024",
    "MaxFileAge": "3",
    "RolloverDirectory": "backup"
  },
  "Swagger": {
    "Title": "FreelanceAI Router API",
    "Version": "v1",
    "Description": "Smart AI request routing service with cost optimization and fallbacks",
    "ContactName": "FreelanceAI Team",
    "ContactEmail": "support@freelanceai.dev"
  }
}
